<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/click"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="back page1" />

    <Button
        android:id="@+id/click2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical|right"
        android:text="into page3" />

    <TextView
        android:id="@+id/liveDataTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="hello livedata" />

    <TextView
        android:id="@+id/liveDatalow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="小写" />


    <TextView
        android:id="@+id/liveDataLen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="长度" />


    <Button
        android:id="@+id/liveDataBtn1"
        android:text="liveDataBtn1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/liveDataBtn2"
        android:text="liveDataBtn2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />


</LinearLayout>
